<UserControl x:Class="BarcodeScanner.MainPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:sdk="http://schemas.microsoft.com/winfx/2006/xaml/presentation/sdk"
    mc:Ignorable="d">

    <Grid x:Name="LayoutRoot" Background="White">
        <StackPanel>
            <Button Name="startStop" Height="35" Width="200" FontSize="18" Margin="0,20,0,20">Scan Bottle UPC</Button>
            <Rectangle Name="rectangle2" Stroke="Black" StrokeThickness="1" VerticalAlignment="Center" HorizontalAlignment="Center" Width="360" Height="270" />
        </StackPanel>
    </Grid>
</UserControl>
